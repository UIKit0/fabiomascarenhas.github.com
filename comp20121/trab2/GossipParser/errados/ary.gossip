class Ary {
  main() {
    var n = 3;

    var x = new Array(n,);
    var y = new Array(n);

    var i = 0;
    while(i < n) {
      x[i] = i + 1;
      i = i + 1;
    }

    var k = 0;
    while(k < 1000) {
      i = n - 1;
      while(0 <= i) {
        y[i] = y[i] + x[i];
        i = i - 1;
      }
      k = k + 1;
    }
    
    out.print(y[0], y[n-1]);
  }
}
